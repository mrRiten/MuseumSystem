<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MuseumSystem.Web</title>
    <link rel="stylesheet" href="~/css/fonts.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/reset.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MuseumSystem.Web.styles.css" asp-append-version="true" />
    
</head>
<body class="page">
    <main class="main" role="main">
        @RenderBody()
    </main>

    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
